{
    "name": "djbdeveloper/laravel-anti-replay",
    "description": "A simple anti-replay middleware for Laravel applications that is compatible with Vue hash routes.",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "middleware", "anti-replay", "csrf", "security", "vue", "hash-routing"],
    "homepage": "https://github.com/djbdeveloper/laravel-anti-replay",
    "authors": [
        {
            "name": "DJB-Developer",
            "email": "djb-developer@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "DJBDeveloper\\LaravelAntiReplay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DJBDeveloper\\LaravelAntiReplay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DJBDeveloper\\LaravelAntiReplay\\Providers\\AntiReplayServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}